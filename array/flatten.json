{
  "name": "flatten",
  "description": "Flattens the nested arrays by a single level.",
  "signature": "<T>(xs: T[]) => T[]",
  "examples": [
    {
      "language": "javascript",
      "content": "flatten([1, [2, 3], 4, [5, 6]]);\n// ⇒ [1, 2, 3, 4, 5, 6]"
    },
    {
      "language": "javascript",
      "content": "flatten([1, [2, [3, 6]], 4, [5, 6]]);\n// ⇒ [1, 2, [3, 6], 4, 5, 6]"
    }
  ],
  "questions": ["How to flatten an array?"]
}
