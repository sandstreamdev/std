{
  "name": "slidingWindow",
  "description": "Returns a new array composed of tuples of the given sliding window length of consecutive elements.",
  "signature": "(count: number) => <T>(xs: T[]) => T[][]",
  "examples": [
    {
      "language": "javascript",
      "content": "slidingWindow(2)([1, 2, 3, 4]);\n// ⇒ [[1, 2], [2, 3], [3, 4]]"
    },
    {
      "language": "javascript",
      "content": "slidingWindow(3)([1, 2, 3, 4, 5]);\n// ⇒ [[1, 2, 3], [2, 3, 4], [3, 4, 5]]"
    },
    {
      "language": "javascript",
      "content": "slidingWindow(1)([1, 2, 3, 4, 5, 6]);\n// ⇒ [[1], [2], [3], [4], [5], [6]]"
    }
  ],
  "questions": ["How to iterate an array pairwise?"]
}
