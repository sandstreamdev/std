{
  "name": "are",
  "description": "Checks if given arguments are all `Arrays`.",
  "signature": "(...xs: any[]) => boolean",
  "examples": [
    {
      "language": "javascript",
      "content": "are([2, 3]); // ⇒ true"
    },
    {
      "language": "javascript",
      "content": "are([1, 2, 3], []); // ⇒ true"
    },
    {
      "language": "javascript",
      "content": "are([1, 2, 3], 8, [1, 3], \"test\"); // ⇒ false"
    }
  ],
  "questions": ["How to check if all the given values are arrays?"]
}
