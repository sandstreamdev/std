  "bugs": {
    "url": "https://github.com/sandstreamdev/std/issues"
  },
  "homepage": "https://github.com/sandstreamdev/std#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.18.2",
    "prettier": "^1.18.2",
    "rollup": "^1.22.0",
    "typescript": "^3.6.3"
  }
}
