{
  "name": "@sandstreamdev/std",
  "version": "0.1.2",
  "description": "",
  "type": "module",
  "module": "index.js",
  "main": "index.cjs.js",
  "browser": "index.umd.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run compile && npm run regenerate && rollup -c && npm run fix",
    "compile": "node --experimental-modules compile.js",
    "fix": "npm run lint:fix",
    "lint": "eslint \"**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "npm run build",
    "prettier": "prettier \"**/*.{js,json,md,ts}\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:fix": "npm run prettier -- --write",
    "regenerate": "node --experimental-modules regenerate.js",
    "test": "npm run lint && node --experimental-modules index.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sandstreamdev/std.git"
  },
